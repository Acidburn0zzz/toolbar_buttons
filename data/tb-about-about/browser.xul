<toolbarbutton
	class="toolbarbutton-1 chromeclass-toolbar-additional"
	id="tb-about-about"
	type="menu"
	viewid="tb-about-about-view"
	onviewshowing="let doc = event.target.ownerDocument; toolbar_buttons.aboutAboutMenu(doc.getElementById('tb-about-about-view'));"
	label="&tb-about-about.label;"
	tooltiptext="&tb-about-about.tooltip;">
		<menupopup id="tb-about-about-view" onpopupshowing="toolbar_buttons.aboutAboutMenu(this);" oncommand="event.stopPropagation();" />
</toolbarbutton>