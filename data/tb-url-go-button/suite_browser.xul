<toolbarbutton
	class="toolbarbutton-1 chromeclass-toolbar-additional"
	id="tb-url-go-button"
	label="&tb-url-go-button.label;"
	tooltiptext="&tb-url-go-button.tooltip;"
	oncommand="window.handleURLBarCommand('none', event);"
    onclick="window.checkForMiddleClick(this, event);"
    ondragover="window.goButtonObserver.onDragOver(event);"
    ondragdrop="window.goButtonObserver.onDrop(event);" />